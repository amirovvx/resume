<main class="ms-0 me-0 ps-0 pe-0">
    <nav class="nav ms-0 ps-0">
        {{#each navigation.links}}
            <div class="nav-item">
                {{#if href}}
                    <a href="{{href}}" class="nav-link active" target="_blank">{{text}}</a> 
                {{else}}    
                    <a href="{{href}}" class="nav-link disabled" target="_blank">{{text}}</a> 
                {{/if}}
            </div>
        {{/each}}
    </nav>
<hr>
<section class="row">
    <div class="col-8">
        <h2 class="mt-3 mb-5">{{header}}</h2>
        {{#with contacts_data}}
            <h3 class="pb-2">{{title}}</h3>
            {{#each buttons}}
                {{#if isPrimary}}
                    <a class="btn btn-warning text-light">{{text}}</a>
                {{else if isOutline}}
                    <a class="btn btn-outline-warning">{{text}}</a>
                {{/if}}
            {{/each}}
            <div class="row mt-3">
                {{#with form.input}}
                    <div class="col-6 pt-2">                
                        {{#with surname}}
                            <label class="form-label">{{label}}</label>
                            <input class="form-control" placeholder="{{placeholder}}">
                            <div class="form-text">{{caption}}</div>
                        {{/with}}
                    </div>
                    <div class="col-6 pt-2">                
                        {{#with name}}
                            <label class="form-label">{{label}}</label>
                            <input class="form-control" placeholder="{{placeholder}}">
                            <div class="form-text">{{caption}}</div>
                        {{/with}}
                    </div>
                    <div class="col-6 pt-4 pb-3">                
                        {{#with phone}}
                            <label class="form-label">{{label}}</label>
                            <input class="form-control" placeholder="{{placeholder}}">
                            <div class="form-text">{{caption}}</div>
                        {{/with}}
                    </div>
                    <div class="col-6 pt-4 pb-3">                
                        {{#with email}}
                            <label class="form-label">{{label}}</label>
                            <input class="form-control" placeholder="{{placeholder}}">
                            <div class="form-text">{{caption}}</div>
                        {{/with}}
                    </div>
                {{/with}}
            </div>
            <hr>
        {{/with}}
        {{#with contacts_data.form}}
            <div class="pb-3 pt-3">
                {{#each switch}}
                    {{#if isActive}}
                        <div class="form-check form-switch mb-2 pt-3">
                            <label class="form-check-label">{{text}}</label>
                            <input name="action_block" type="radio" class="form-check-input">
                        </div>
                    {{else}}
                        <div class="form-check form-switch mb-2 pt-3">
                            <label class="form-check-label">{{text}}</label>
                            <input name="action_block" type="radio" class="form-check-input">
                        </div>
                    {{/if}}
                {{/each}}
            </div>
        {{/with}}
        <hr>
    </div>
    <div class="col-4">
        <br>
        <br>
        <br>
        <br>
        <div class="card mt-3 mb-3">
            <div class="card-body p-2">
                {{#each action_block}}
                    <div class="m-1">
                        {{#if isSuccess}}
                            <a class="btn btn-success">{{text}}</a>
                        {{else isDanger}}
                            <a class="btn btn-danger">{{text}}</a>
                        {{else isInfo}}
                            <a class="btn btn-info">{{text}}</a>
                        {{/if}}
                    </div>
                {{/each}}
            </div>
        </div>
        <div class="card">
            <div class="card-body">
                {{#with total}}
                    <h4>{{title}}</h4>
                    {{#each item_list}}
                        <div>{{name}}: {{price}}</div>
                    {{/each}}
                    <hr>
                    {{#each additional_info}}
                        <div>{{text}}: {{value}}</div>
                    {{/each}}
                    <hr>
                    {{#with total_price}}
                        <div>{{text}}: {{value}}</div>
                    {{/with}}
                    <hr>
                    {{#each button}}
                        <div class="pb-2">
                            {{#if isPrimary}}
                                <a class="btn btn-sm btn-primary">{{text}}</a>
                            {{else isOutline}}
                                <a class="btn btn-sm btn-outline-primary">{{text}}</a>
                            {{/if}}
                        </div>   
                    {{/each}}
                {{/with}}
            </div>
        </div>
    </div>
    <div class="col-8">
        {{#with delivery}}
            <h3>{{title}}</h3>
            <div class="card">
                <div class="row">                    
                    {{#each select}} 
                        <div class="card-body col-6">                   
                            <label class="form-label">{{label}}</label>
                            <select class="form-select form-select-sm">
                            <option disabled selected>{{placeholder}}</option>
                            {{#each options}}
                                <option value="{{this.value}}">{{this.text}}</option>
                            {{/each}}
                            </select>
                        </div>
                    {{/each}}
                </div>
            </div>
            {{#each delivery_type}}
                <div class="form-check mb-3 mt-3">
                    <label class="form-check-label">{{type}}</label>
                    <input type="radio" name="action_block" class="form-check-input">
                </div>
            {{/each}}
            <hr>
            {{#with payment}}
                <h3>{{title}}</h3>
                {{#each list}}
                    <div class="form-check mb-3 mt-3">
                        <label class="form-check-label">{{type}}</label>
                        <label class="form-check-label">{{description}}</label>
                        <input type="radio" name="action_block" class="form-check-input">
                    </div>
                {{/each}}
            {{/with}}
            <hr>
            <div class="row mt-3">
                {{#with contact}}
                    <h3 class="pb-2">{{title}}</h3>
                    <div class="col-6 pt-2">                
                        {{#with surname}}
                            <label class="form-label">{{label}}</label>
                            <input class="form-control" placeholder="{{placeholder}}">
                            <div class="form-text">{{caption}}</div>
                        {{/with}}
                    </div>
                    <div class="col-6 pt-2">                
                        {{#with name}}
                            <label class="form-label">{{label}}</label>
                            <input class="form-control" placeholder="{{placeholder}}">
                            <div class="form-text">{{caption}}</div>
                        {{/with}}
                    </div>
                    <div class="col-6 pt-4 pb-3">                
                        {{#with phone}}
                            <label class="form-label">{{label}}</label>
                            <input class="form-control" placeholder="{{placeholder}}">
                            <div class="form-text">{{caption}}</div>
                        {{/with}}
                    </div>
                    <div class="col-6 pt-4 pb-3">                
                        {{#with email}}
                            <label class="form-label">{{label}}</label>
                            <input class="form-control" placeholder="{{placeholder}}">
                            <div class="form-text">{{caption}}</div>
                        {{/with}}
                    </div>
                {{/with}}
            </div>
            <hr>
            {{#with coment}}
                <label class="form-label">{{label}}</label>
                <input class="form-control" placeholder="{{placeholder}}">
                <br>
            {{/with}}
        {{/with}}
    </div>   
    <div class="col-8">
        <hr>
        {{#with coupon}}
            <h3 class="mb-3">{{title}}</h3>
            {{#with select}}
                {{#with availabel_coupon}}
                    <label class="form-label">{{label}}</label>
                    <select class="form-select form-select-lg mb-3">
                        <option disabled selected>{{placeholder}}</option>
                        {{#each options}}
                            <option value="{{this.value}}">{{this.text}}</option>
                        {{/each}}
                    </select>                    
                {{/with}}
                {{#with friend_coupon}}
                    <label class="form-label">{{label}}</label>
                    <select class="form-select form-select-sm">
                        <option disabled selected>{{placeholder}}</option>
                        {{#each options}}
                            <option value="{{this.value}}">{{this.text}}</option>
                        {{/each}}
                    </select>                   
                {{/with}}            
            {{/with}}
            {{#with add}}
                {{#with friend}}
                    <label class="form-label mt-3">{{label}}</label>
                    <input class="form-control form-control-lg" placeholder="{{placeholder}}">
                    <div class="form-text">{{caption}}</div>
                {{/with}}
                {{#with colleague}}
                    <label class="form-label mt-3">{{label}}</label>
                    <input class="form-control form-control-sm" placeholder="{{placeholder}}">
                    <div class="form-text mb-4">{{caption}}</div>
                {{/with}}
            {{/with}}
        {{/with}}
    </div>
    <div class="col-8">
        <hr class="mb-4">
        {{#with quiz}}
            <h4 class="mb-4">{{title}}</h4>
            {{#with preference}}
                <div class="form-label">{{title}}</div>
                {{#each options}}
                    <div class="form-check mb-3 mt-3">
                        <label class="form-check-label">{{this}}</label>
                        <input type="radio" name="options" class="form-check-input">
                    </div>
                {{/each}}        
            {{/with}}
            <label class="form-label mt-2">{{your_expirience.label}}</label>
            <input class="form-control">
            {{#with purchase_frequency}}
                <label class="form-label mt-3">{{label}}</label>
                <select class="form-select">
                    <option disabled selected>{{placeholder}}</option>
                    {{#each options}}
                        <option value="{{this.value}}">{{text}}</option>
                    {{/each}}
                </select>
            {{/with}}
            {{#with factors}}
                <label class="form-label text-sm mt-4">{{title}}</label>
                {{#each options}}
                    <div class="form-check mb-3 mt-3">
                        <label class="form-check-label">{{this}}</label>
                        <input type="checkbox" name="options" class="form-check-input">
                    </div>
                {{/each}}
            {{/with}}
        {{/with}}
    </div>
</section>
<hr class="mt-5">
<footer class="ms-0 me-0">
    <div class="nav">
        {{#each footer}}
            <nav class="nav-item col-2 my-2 ps-0 pe-0 mt-0 ms-0 me-0">
                {{#each this}}
                    <a href="{{link}}" target="_blank" class="nav-link active">{{text}}</a>
                {{/each}}
                <a href="#" class="nav-link disabled">Disabled</a>
            </nav>
        {{/each}}
    </div>
</footer>
</main>