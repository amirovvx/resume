<main>
    <nav>
        {{#with navigation}}
            <div class="row">
                {{#each links}}
                    <div class="col-2">                                    
                        <a href="{{href}}" target="_blank">{{text}}</a>
                    </div>        
                {{/each}}
                <div class="col-2">
                    <button class="btn btn-primary btn-sm" href="{{button.href}}" target="_blank">{{button.text}}</button>
                </div>
            </div>
        {{/with}}
    </nav>
    <hr>
    <header>
        {{#with header}}
        <br>
            <h1>{{title}}</h1>
            <p>"{{description}}"</p>
            <a href="{{button.link}} " target="_blank" class="btn btn-primary">{{button.text}}</a>   
        {{/with}}
        <br>
        <br>
    </header>
    <hr>
    <br>
    <section>
        {{#with city}}
            <h2>{{title}}</h2>
            <div class="row g-3">
                {{#each cards}}
                    <section class="col-6">        
                        <div class="card">
                            <img class="card-img-top" src="{{picture.src}}" alt="{{picture.alt}}">
                            <div class="card-body">
                                <h5 class="card-title">{{title}}</h5>
                                <p>{{description}}</p>
                                <a class="btn btn-primary" href="{{button.href}}" target="_blank">{{button.text}}</a>
                            </div>
                        </div>
                    </section>
                {{/each}}
            </div>
        {{/with}}    
    </section>
    <hr>
    <br>
    <section>
        {{#with winner}}
            <h2>{{title}}</h2>           
            <table class="table">
                {{#with table}}
                    <thead>
                        {{#each head}}
                            <th>{{this}}</th>
                        {{/each}}
                    </thead>
                    <tbody>
                        {{#each user}}
                            <tr>
                                <th>{{place}}</th>
                                <td>{{name}}</td>
                                <td>{{surname}}</td>
                                <td>{{nickname}}</td>
                            </tr>
                        {{/each}}
                    </tbody>
                {{/with}}
            </table>            
        {{/with}}
    </section>
    <hr>
    <br>
    <section>
        {{#with gallery}}
            <h2>{{title}}</h2>
            <br>
            <div class="row gx-2 gy-2">
                {{#each cards}}
                    <div class="list-group col-4">
                        <div class="card">
                            <p class="card-header">
                                {{name}}
                            </p>
                            {{#each items}}
                                <div class="list-group-item">{{this}}</div>
                            {{/each}}
                        </div>
                    </div>
                {{/each}}
            </div>
        {{/with}}
    </section>
    <hr>
    <br>
    <section>
        {{#with relaxion}}
            <p class="display-3">{{title}}</p>
            <br>
            <div class="row gx-2 gy-4">
                {{#each cards}}
                    <div class="col-5">
                        <div class="card text bg-secondary text-light">
                            <div class="card-body">
                                <p class="card-header">{{header}}</p>
                                <p><h6 class="card-title">{{title}}</h6></p>
                                <p>"{{description}}"</p>
                            </div>
                        </div>
                    </div>
                {{/each}}
            </div>
        {{/with}}
    </section>
    <hr>
    <br>
    <section>        
        <div class="row g-2">
            {{#each rest.cards}}
                <div class="col-6"> 
                    <div class="card ">
                        <p class="card-header text-center">{{header}}</p>
                        <div class="card-body text-center">
                            <p><h6 class="card-title">{{title}}</h6></p>
                            <p>"{{description}}"</p>
                            <button class="btn btn-primary">{{button}}</button>
                        </div>
                        <div class="card-footer text-center">{{footer}}</div>
                    </div>
                </div>
            {{/each}}
        </div>
    </section>
    <br>
    <hr>
    <br>
    <section>
        {{#with proposition}}
            <p class="display-2">{{title}}</p>
            <br>
            <div class="row g-2">
                {{#each card}}
                    <div class="col-5"> 
                        <div class="card bg-warning text-light">
                            <p class="card-header">{{header}}</p>
                            <div class="card-body">
                                <h6 class="card-title">{{title}}</h6>
                                <p>"{{description}}"</p>
                            </div>
                        </div>
                    </div>
                {{/each}}
            </div>
        {{/with}}
    </section>
    <hr>
    <footer>
        {{#with footer}}
            <p><h5>{{title}}</h5></p>
            <div class="row">
                <div class="col-3">
                    {{#each navigation}}
                        <div class="my-3">                                  
                            <a class="btn btn-link" href="{{href}}" target="_blank">{{text}}</a>
                        </div>
                    {{/each}}
                </div>
                <div class="col-3">
                    {{#each socialMedia}} 
                        <div class="my-3">                               
                            <a class="btn-sm btn btn-outline-dark" href="{{link}}" target="_blank">{{name}}</a> 
                        </div>
                    {{/each}}
                </div>
                <div class="col-3">
                    {{#each partners}}
                        <div class="my-3">
                            <a class="btn-sm btn btn-outline-info" href="{{link}}" target="_blank">{{name}}</a>
                        </div>
                    {{/each}}
                </div>
            </div>                
        {{/with}}
    </footer>
</main>